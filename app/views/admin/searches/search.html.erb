<div class="container px-sm-0 px-5" >
  <% if @range == "会員"%>
    <div class="row my-4">
      <h3>会員検索結果</h3>
    </div>
    <div class="row">
      <%= render 'search_customer', customers: @records%>
    </div>
  <% else %>
    <div class="row my-4">
      <h3>商品検索結果</h3>
    </div>
    <div class="row">
      <%= render 'search_item', items: @records%>
    </div>
  <% end %>
</div>
