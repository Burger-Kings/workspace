<div class="genre_search">
  <table class="table">
    <tr>
      <th>ジャンル検索</th>
    </tr>

    <%= form_with url: search_items_path, method: :get do |f| %>
      <% @genres.each do |genre| %>
        <tr>
          <td><%= link_to genre.name, search_items_path(genre_id: genre.id), method: :get %></td>
        </tr>
      <% end %>
    <% end %>

  </table>
</div>